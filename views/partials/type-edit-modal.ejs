<div id="typeEditModal" class="modal-container">
  <div class="modal">
    <div class="modal-head">
      <div class="modal-title">Edit type</div>
      <div id="typeEditModalClose" class="modal-close">
        <%- include("../partials/icons/close-svg"); -%>
      </div>
    </div>
    <div class="modal-content">
      <div class="container">
        <form action="/options/edit-leave-type" method="post">
          
      <% leaveTypeList.map((lv,idx) =>{ %>
          <label for="projectDetails">
            <%= idx + 1 %>
            <div class="">
              <input type="text"  value="<%= lv.name %>" name="<%= lv.id %>" />
              <p class="error start-error"></p>
            </div>
          </label>
          <% }) %>
          <div class="">
            <button class="submit-btn">Save</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
