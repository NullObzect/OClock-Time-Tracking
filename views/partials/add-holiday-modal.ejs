<div id="myModal" class="modal-container">
  <div class="modal">
    <div class="modal-head">
      <div class="modal-title">Add Holiday</div>
      <div class="modal-close">
        <%- include("../partials/icons/close-svg"); -%>
      </div>
    </div>
    <div class="modal-content">
      <div class="container">
        <form class="form" action="/add-holiday" method="post">
          <label for="name"
            >Holiday Name
            <div class="">
              <input id="name" name="title" type="text" />
              <p class="error title-error"></p>
            </div>
          </label>
          <label for="projectDetails">
            Select Start Date
            <div class="">
              <input type="date" name="start" />
              <p class="error start-error"></p>
            </div>
          </label>
          <label for="projectDetails">
            Select End Date
            <div class="">
              <input type="date" name="end" />
              <p class="error end-error"></p>
            </div>
          </label>
          <div class="">
            <button class="submit-btn">Save</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
