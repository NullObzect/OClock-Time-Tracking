<div id="myModal" class="modal-container">
  <div class="modal">
    <div class="modal-head">
      <div class="modal-title">Contact</div>
      <div class="modal-close">
        <%- include("../partials/icons/close-svg"); -%>
      </div>
    </div>
    <div class="modal-content">
      <div class="container">
        <form action="/options/contact-admin" method="post">
          <label for="name"
            >Person
            <div class="">
              <select required name="userId" id="projectName">
                <option value="admin">Admin</option>
              </select>
            </div>
          </label>
          <label for="projectDetails">
            Notice Details
            <div class="">
              <textarea required
                name="noticeDetails"
                id="projectDetails"
                cols="30"
                rows="3"
              ></textarea>
            </div>
          </label>
          <div class="">
            <button class="submit-btn" type="submit">Send</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
