<div id="myModal" class="modal-container">
  <div class="modal">
    <div class="modal-head">
      <div class="modal-title">Add Payroll</div>
      <div class="modal-close">
        <%- include("../partials/icons/close-svg"); -%>
      </div>
    </div>
    <div class="modal-content">
      <div class="container">
        <form id="manualForm" action="/add-payroll" method="post">
          <label for="name">
            <div class="">
              <span class="isExist-attdance"></span>
              <select name="userId" id="userId">
                <option disabled selected>Select Employee</option>
                <% users.map(u=>{ %>
                <option value="<%= u.id %>"><%= u.user_name %></option>

                <% }) %>
              </select>
            </div>
          </label>
          <p class="error employee-error"></p>

          <label for="end">
            Amount
            <div class="">
              <input
                type="number"
                class="manual-end-time"
                placeholder="add amount"
                name="amount"
                required
              />
              <p class="error endTime-error"></p>
            </div>
          </label>

          <div class="">
            <button id="submit-btn" class="submit-btn">Submit</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
